    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true,
    "node": true,
    "shared-node-browser": true
  },
  "extends": [
    "./rules/01_possible-errors.json",
    "./rules/02_best-practices.json",
    "./rules/03_variables.json",
    "./rules/04_stylistic-issues.json",
    "./rules/05_es6.json",
    "./rules/06_imports.json",
    "./rules/07_eslint_comments.json",
    "./rules/08_promise.json",
    "./rules/09_unicorn.json",
    "./rules/10_react.json",
    "./rules/11_typescript.json",
    "./rules/12_exotic.json",
    "./rules/99_prettier.json"
  ],
  "reportUnusedDisableDirectives": true,
  "rules": {}
}
