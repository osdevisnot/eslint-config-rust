{
  "plugins": ["unicorn"],
  "extends": ["plugin:unicorn/recommended"],
  "rules": {
    "unicorn/prevent-abbreviations": "off",
    "unicorn/no-keyword-prefix": "error",
    "unicorn/no-unsafe-regex": "error",
    "unicorn/prefer-string-replace-all": "error",
    "unicorn/filename-case": [
      "error",
      {
        "cases": {
          "pascalCase": true,
          "kebabCase": true,
          "camelCase": true
        }
      }
    ]
  }
}
